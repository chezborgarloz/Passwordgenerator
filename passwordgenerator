import random


lower = "abcdefghijklmnopqrstuvxyz"
upper = "ABCDEFGHIJKLMNOPQRSTUVWXYZ"
numbers = "0123456789"
symbols = "!@#$%^&*([]{})"


all = lower + upper + numbers + symbols
length = 16
password = "".join(random.sample(all,length))


print(password)

import random
word = ("Thanks for using our program :)","Easy to use, harder to hack","Hackers won't be able to hack this password")
print("")
print(random.choice(word))
